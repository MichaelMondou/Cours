package hex;

public class Main {

	public static void main(String[] args) {
		
		

		Window window = new Window("Hex", 640, 720);
		
		Modèle model = new Modèle();
		Contrôleur controller = new Contrôleur(model);
		
		VueHex view = new VueHex(window, model, controller);
		
		model.addObserver(view);
	}
}
