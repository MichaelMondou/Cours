package mvc;

import javax.swing.JButton;
import javax.swing.JLabel;
import javax.swing.JPanel;

public abstract class View_Button extends View{

    /**
     * Conteneur de tous les éléments
     */
    private JPanel panel;	
	/**
	 * Bouton augmentant la température
	 */
	private JButton up;
	/**
	 * Bouton baissant la température
	 */
    private JButton down;
    /**
     * Texte indiquant l'unité de la valeur
     */
    private JLabel titre;
    
	public JButton getUp() {
		return up;
	}
	public void setUp(JButton up) {
		this.up = up;
	}
	public JButton getDown() {
		return down;
	}
	public void setDown(JButton down) {
		this.down = down;
	}
	public JPanel getButtons() {
		return panel;
	}
	public void setButtons(JPanel panel) {
		this.panel = panel;
	}
	
}
