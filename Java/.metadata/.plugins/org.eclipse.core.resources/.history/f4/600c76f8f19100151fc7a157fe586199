package hex;

public class Main {

	public static void main(String[] args) {
		
		

		Fenêtre fenetre = new Fenêtre("Hex", 640, 720);
		
		Modèle modele = new Modèle();
		Contrôleur controller = new Contrôleur(modele);
		
		VueHex view = new VueHex(fenetre, modele, controller);
		
		modele.addObserver(view);
	}
}
