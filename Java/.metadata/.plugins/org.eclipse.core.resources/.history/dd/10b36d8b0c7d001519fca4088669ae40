package mvc;

public class Main {
	public static void main(String args[]) {
		Model model = new Model();
		TemperatureController tempcontrolC = new TemperatureController(model);
		TemperatureController tempcontrolF = new TemperatureController(model);
		TemperatureVueCelsius pvc = new TemperatureVueCelsius(model, tempcontrolC, 100, 200);
		tempcontrolC.addView(pvc);
	}
}
