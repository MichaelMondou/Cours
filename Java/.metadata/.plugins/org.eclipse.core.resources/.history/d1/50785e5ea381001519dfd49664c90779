package hex;

import java.awt.event.MouseEvent;

public class Controller {
	
	private Model model;

	public Controller(Model model) {
		this.model = model;
	}
	
	public void Action(View object, Event event) {
		switch (event) {
		case MouseEvent.MOUSE_CLICKED
			break;
		default:
			break;
		}
	}

}
